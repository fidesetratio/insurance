<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="elions.muamalat">

	<typeAlias alias="hashMap" type="java.util.HashMap" />
	<typeAlias alias="bmi" type="com.ekalife.elions.model.Bmi"/>

	<update id="updateMstDataBmi" parameterClass="bmi">
		UPDATE eka.mst_data_bmi                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
		<dynamic prepend=" set ">                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
			<isNotNull removeFirstPrepend="true" prepend="," property="flag_debet">flag_debet = #flag_debet# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                       
			<isNotNull removeFirstPrepend="true" prepend="," property="jenis">jenis = #jenis# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                                      
			<isNotNull removeFirstPrepend="true" prepend="," property="lsbp_id">lsbp_id = #lsbp_id# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                                
			<isNotNull removeFirstPrepend="true" prepend="," property="nama">nama = upper(#nama#) </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                                  
			<isNotNull removeFirstPrepend="true" prepend="," property="no_seri">no_seri = upper(#no_seri#) </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                         
			<isNotNull removeFirstPrepend="true" prepend="," property="norek">norek = upper(#norek#) </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                               
			<isNotNull removeFirstPrepend="true" prepend="," property="posisi">posisi = #posisi# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                                   
			<isNotNull removeFirstPrepend="true" prepend="," property="premi">premi = #premi# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                                      
			<isNotNull removeFirstPrepend="true" prepend="," property="premi_ke">premi_ke = #premi_ke# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                             
			<isNotNull removeFirstPrepend="true" prepend="," property="reg_spaj">reg_spaj = upper(#reg_spaj#) </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                      
			<isNotNull removeFirstPrepend="true" prepend="," property="tahun_ke">tahun_ke = #tahun_ke# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                             
			<isNotNull removeFirstPrepend="true" prepend="," property="tgl_debet">tgl_debet = #tgl_debet# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                          
			<isNotNull removeFirstPrepend="true" prepend="," property="tgl_input">tgl_input = #tgl_input# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                          
			<isNotNull removeFirstPrepend="true" prepend="," property="tgl_lahir">tgl_lahir = #tgl_lahir# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                          
			<isNotNull removeFirstPrepend="true" prepend="," property="tgl_proses">tgl_proses = #tgl_proses# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                       
			<isNotNull removeFirstPrepend="true" prepend="," property="user_input">user_input = #user_input# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                       
			<isNotNull removeFirstPrepend="true" prepend="," property="user_proses">user_proses = #user_proses# </isNotNull>                                                                                                                                                                                                                                                                                                                                                                                                    
		</dynamic>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
		WHERE MDB_ID = #mdb_id#                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
	</update>
	<!--  
		WHERE reg_spaj = RPAD (#reg_spaj#, 11, ' ')
		  AND premi_ke = #premi_ke#
		  AND tahun_ke = #tahun_ke#                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 	
	-->
	
</sqlMap>