<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap>

    <delete id="elions.snows.deleteMstInboxDet" parameterClass="java.lang.String">
        DELETE FROM eka.MST_INBOX_DET
        WHERE mi_id = #mi_id#
    </delete>
  
     <delete id="elions.snows.deleteMstInboxHist" parameterClass="java.lang.String">
        DELETE FROM eka.mst_inbox_hist 
        WHERE mi_id = #mi_id#
    </delete>
    
    <delete id="elions.snows.deleteMstInbox" parameterClass="java.lang.String">
        DELETE FROM eka.mst_inbox 
        WHERE mi_id = #mi_id#
    </delete>
    
</sqlMap>