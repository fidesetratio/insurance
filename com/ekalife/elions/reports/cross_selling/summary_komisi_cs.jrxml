	<queryString>
		<![CDATA[SELECT   a.reg_spaj, a.mscs_policy_no, a.mscs_prod_date, a.mscs_pay_date,
         b.msag_id, d.mcl_first agen, c.msag_tabungan, k.lsbp_nama,
         j.lbn_nama, e.lsbs_id, e.lsbs_name, f.lsdbs_number, f.lsdbs_name,
         i.lku_symbol, a.mscs_premium, h.lca_nama, a.mscs_holder, a.mscs_tsi,
         g.lus_login_name, a.mscs_comm, a.mscs_comm_tax, a.mscs_comm_paid, a.mscs_comm_kurs
    FROM eka.mst_policy_cs a,
         eka.mst_agent_cs b,
         eka.mst_agent c,
         eka.mst_client_new d,
         eka.lst_bisnis e,
         eka.lst_det_bisnis f,
         eka.lst_user g,
         eka.lst_cabang h,
         eka.lst_kurs i,
         eka.lst_bank j,
         eka.lst_bank_pusat k
   WHERE a.reg_spaj = b.reg_spaj
     AND b.lsle_id = 4
     AND b.msag_id = c.msag_id
     AND c.mcl_id = d.mcl_id
     AND a.lsbs_id = e.lsbs_id
     AND a.lsbs_id = f.lsbs_id
     AND a.lsdbs_number = f.lsdbs_number
     AND a.lus_id = g.lus_id
     AND c.lca_id = h.lca_id
     AND a.lku_id = i.lku_id
     AND c.lbn_id = j.lbn_id
     AND j.lsbp_id = k.lsbp_id
     AND g.lus_id = $P{lus_id}
     AND a.mscs_prod_date BETWEEN TO_DATE (TO_CHAR ($P{tanggalAwal}, 'dd/MM/yyyy') || ' 00:00:00', 'dd/MM/yyyy hh24:mi:ss')
                              AND TO_DATE (TO_CHAR ($P{tanggalAkhir}, 'dd/MM/yyyy') || ' 23:59:59', 'dd/MM/yyyy hh24:mi:ss')
ORDER BY h.lca_nama, a.mscs_input_date]]>
