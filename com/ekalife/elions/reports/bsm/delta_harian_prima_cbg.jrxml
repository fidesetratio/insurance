	<queryString>
		<![CDATA[SELECT 
    EKA.UTILS.CABANG_BSM(A.WIL_NO) WILAYAH, 
    EKA.UTILS.CABANG_BSM(A.HEAD_NO) CABANG_INDUK, 
    A.NAMA_CABANG, 
    NVL( B.MASUK, 0 ) masuk, NVL( C.KELUAR , 0) CAIR,
    eka.utils.kurs_simbol ( $P{lku_id} ) lku_symbol,
    ( NVL( B.MASUK, 0 ) - NVL( C.KELUAR , 0)) delta
FROM EKA.LST_CABANG_BII A,
(
  SELECT   c.lcb_no,
           SUM (d.mpr_deposit) masuk
    FROM   eka.mst_policy p,
           eka.mst_reff_bii r,
           eka.lst_cabang_bii c,
           eka.mst_powersave_ro d
   WHERE       p.lca_id = '09' AND mspo_policy_no is not null
           AND p.lspd_id <> 95
           AND p.lku_id = $P{lku_id} --parameter
           AND p.reg_spaj = r.reg_spaj
           AND r.no_urut = (SELECT MAX (no_urut) FROM eka.mst_reff_bii WHERE reg_spaj = r.reg_spaj)
           AND r.lcb_no = c.lcb_no
           AND c.lcb_no = rpad($P{lcb_no}, 5, ' ') --parameter
           AND c.jenis in (2,16)
           AND p.reg_spaj = d.reg_spaj
           AND d.mps_deposit_date BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}  --parameter 
           AND d.mps_kode = 5
GROUP BY   c.lcb_no
) B,           
(
  SELECT   c.lcb_no,
           SUM (e.keluar) keluar
    FROM   eka.mst_policy p,
           eka.mst_reff_bii r,
           eka.lst_cabang_bii c,
           eka.mst_powersave_ro d,
           (		SELECT   a.reg_spaj, b.mpr_deposit keluar
                  FROM   eka.mst_pinjaman a, eka.mst_powersave_ro b
                 WHERE   a.flag_ps = 1 and a.reg_spaj = b.reg_spaj and b.mps_kode = 5
                         AND (a.mspin_tgl_keluar BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}) 
                UNION
                SELECT   a.reg_spaj, b.mpr_deposit keluar
                  FROM   eka.mst_surrender a, eka.mst_powersave_ro b
                 WHERE   a.flag_ps = 1 and a.reg_spaj = b.reg_spaj and b.mps_kode = 5
                         AND (a.mssur_tgl_klaim BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir} ) --parameter 
                UNION
                SELECT   a.reg_spaj, b.mpr_deposit keluar
                  FROM   eka.mst_maturity a, eka.mst_powersave_ro b
                 WHERE   a.flag_ps = 1 and a.reg_spaj = b.reg_spaj and b.mps_kode = 5
                         AND (a.tgl_klaim BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir} )
) e --parameter 
   WHERE       p.lca_id = '09'
           AND p.lspd_id <> 95
           AND p.lku_id = $P{lku_id} --parameter
           AND p.reg_spaj = r.reg_spaj
           AND r.no_urut = (SELECT MAX (no_urut) FROM eka.mst_reff_bii WHERE reg_spaj = r.reg_spaj)
           AND r.lcb_no = c.lcb_no
           AND c.lcb_no = rpad($P{lcb_no}, 5, ' ') --parameter
           AND c.jenis in (2,16)
           AND p.reg_spaj = d.reg_spaj
           AND d.mpr_aktif = 0
           AND d.reg_spaj = e.reg_spaj
           AND d.mps_kode = 5
GROUP BY   c.lcb_no
) C
WHERE A.JENIS in (2,16)
   AND a.lcb_no = rpad($P{lcb_no}, 5, ' ')  --parameter
  AND A.FLAG_AKTIF = 1
  AND A.level_id > 1
  AND A.LCB_NO = B.LCB_NO(+)
  AND A.LCB_NO = C.LCB_NO(+)
ORDER BY     
    EKA.UTILS.CABANG_BSM(A.WIL_NO), 
    EKA.UTILS.CABANG_BSM(A.HEAD_NO), 
    A.NAMA_CABANG]]>
	</queryString>
