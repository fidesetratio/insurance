	</parameter>
	<queryString>
		<![CDATA[SELECT sl.msl_tu_ke, sl.msl_desc, p.lku_id, p.mspo_nasabah_dcif, p.mspo_policy_no_format, p.reg_spaj, cb.nama_cabang,
       sl.msl_premi, pi.mspr_tsi, sl.msl_mgi, sl.msl_rate, sl.msl_premi, sl.msl_bunga,
       (sl.msl_premi + sl.msl_bunga) hasil_mgi, p.mspo_ins_period, p.mspo_date_print, sl.msl_bdate, i.mste_tgl_aksep,
       eka.utils.user_input (p.lus_id) lus_login_name, 
       eka.utils.kurs_simbol (p.lku_id) lku_symbol,
       eka.utils.pemegang (p.reg_spaj) pemegang,
       eka.utils.tertanggung (p.reg_spaj) tertanggung,
       eka.utils.cabang_bsm (cb.head_no) cabang_induk,
       eka.utils.cabang_bsm (cb.wil_no) wilayah,
       eka.utils.produk (pi.lsbs_id) lsbs_name,
       eka.utils.det_produk (pi.lsbs_id, pi.lsdbs_number) lsdbs_name,
       eka.utils.agen_bancass(rb.lrb_id) agen_penutup,
       eka.utils.agen_bancass(rb.reff_id) agen_referal,
       eka.utils.no_rek(p.reg_spaj) no_rek_bsm,
       cair.tgl_cair, cair.nominal_cair, 
       decode(pin.reg_spaj, null, cair.nominal_cair, round((cair.nominal_cair  * 0.8), 2)) cair_penarikan , 
       decode(pin.reg_spaj, null, 0, cair.nominal_cair -  round((cair.nominal_cair * 0.8), 2)) cair_pinjaman,
       cair.mslb_no_reff,
       cb.lcb_no
  FROM eka.mst_policy p,
       eka.mst_insured i,
       eka.mst_product_insured pi,
       eka.mst_reff_bii rb,
       eka.lst_cabang_bii cb,
       eka.mst_slink sl,
       (SELECT   reg_spaj, mslb_tu_ke, mslb_paid_date tgl_cair, mslb_jum_bayar nominal_cair, mslb_no_reff
        FROM   eka.mst_slink_bayar
        WHERE   (mslb_paid_date BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}) --parameter
        AND mslb_desc = 'Penarikan' ) cair,
        eka.mst_pinjaman pin
 WHERE p.lspd_id <> 95
   AND p.reg_spaj = i.reg_spaj
   AND p.lku_id = $P{lku_id} --parameter
   AND i.reg_spaj = pi.reg_spaj
   AND i.mste_insured_no = pi.mste_insured_no
   AND pi.lsbs_id < 300
   AND p.reg_spaj = rb.reg_spaj
   AND p.reg_spaj = rb.reg_spaj
   and cair.reg_spaj = pin.reg_spaj (+)
   and cair.mslb_no_reff = pin.mspin_no_pinjaman (+)
   AND rb.level_id = 4
   AND rb.no_urut = (SELECT MAX (no_urut) FROM eka.mst_reff_bii WHERE reg_spaj = rb.reg_spaj)
   AND rb.lcb_no = cb.lcb_no
   AND cb.lcb_no = rpad($P{lcb_no}, 5, ' ') --parameter
   AND cb.jenis = 2
   AND p.reg_spaj = sl.reg_spaj
   AND sl.msl_kode = 5
   AND exists (select 0 from eka.mst_production where reg_spaj = p.reg_spaj and mspro_prod_ke = 1)
   AND sl.reg_spaj = cair.reg_spaj
   AND sl.msl_tu_ke = cair.mslb_tu_ke
   AND cb.flag_aktif = 1
   AND cb.level_id > 1
ORDER BY wilayah, cabang_induk, nama_cabang, reg_spaj, msl_tu_ke]]>
	</queryString>
