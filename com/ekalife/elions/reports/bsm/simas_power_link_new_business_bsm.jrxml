	<variable name="feebase" class="java.math.BigDecimal">
		<variableExpression><![CDATA[new BigDecimal($F{PREMI_POKOK}.intValue() * 0.5)]]></variableExpression>
	</variable>
	<variable name="feetopup" class="java.math.BigDecimal">
		<variableExpression><![CDATA[new BigDecimal(($F{PREMI_TOP_UP}.intValue() * 2) / 100)]]></variableExpression>
	</variable>
	<variable name="tot_feebase" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[new BigDecimal($F{PREMI_POKOK}.intValue() )]]></variableExpression>
	</variable>
	<variable name="tot_fee_topup" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[new BigDecimal($F{PREMI_TOP_UP}.intValue())]]></variableExpression>
	</variable>
	<variable name="tot_premi" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[new BigDecimal($F{TOTAL_PREMI}.intValue())]]></variableExpression>
	</variable>
