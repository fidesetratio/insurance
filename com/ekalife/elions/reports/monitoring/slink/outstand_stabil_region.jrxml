	<parameter name="props" class="java.util.Properties"/>
	<queryString>
		<![CDATA[SELECT 
    eka.utils.cabang_bsm(b.wil_no) cabang, sum(decode(lku_id, '02', 9600, 1) * saldo_awal) saldo_awal, sum(decode(lku_id, '02', 9600, 1) * saldo_akhir) saldo_akhir,
    (select sum(jumlah_target)total from eka.lst_target_cabang where lcb_no in (select lcb_no from eka.lst_cabang_bii where wil_no=b.wil_no) and lsbs_id =164)target_cabang,
    (SELECT 
        sum(decode(lku_id, '02', 9600, 1) * saldo_akhir) saldo_akhir
    FROM 
        eka.mst_powersave_monthly a, eka.lst_cabang_bii b 
    WHERE 
        periode between TRUNC(to_date($P{tgl_b},'dd/MM/yyyy') ,'MONTH') and LAST_DAY(TRUNC(to_date($P{tgl_b},'dd/MM/yyyy'),'MONTH')) AND lsbp_id = 156
        AND nama_plan = 'STABLE LINK NEW'
        AND a.lcb_id = b.lcb_no)total_saldo_akhir
FROM 
    eka.mst_powersave_monthly a, eka.lst_cabang_bii b 
WHERE 
    periode between TRUNC(to_date($P{tgl_b},'dd/MM/yyyy') ,'MONTH') and LAST_DAY(TRUNC(to_date($P{tgl_b},'dd/MM/yyyy'),'MONTH')) AND lsbp_id = 156
    AND nama_plan = 'STABLE LINK NEW'
    AND a.lcb_id = b.lcb_no
GROUP BY b.wil_no
ORDER BY saldo_akhir desc]]>
	</queryString>
