	<parameter name="lsdbsNumber" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT eka.utils.produk(a.lsbs_id) produk, eka.utils.det_produk(a.lsbs_id, a.lsdbs_number) det_produk, sum(case when mspe_sex = 0 then 1 else 0 end) cowo, sum(case when mspe_sex = 1 then 1 else 0 end) cewe
FROM
    eka.mst_product_insured a,
    (SELECT 
        a.reg_spaj, e.mspe_sex
    FROM 
        eka.mst_production a, eka.mst_det_production b, eka.mst_policy c, eka.mst_reff_bii d, eka.mst_client_new e
    WHERE 
        a.lstb_id = 1 
        AND a.mspro_prod_date between to_date($P{tgl_a},'dd/MM/yyyy') and to_date($P{tgl_b},'dd/MM/yyyy')
        AND a.lstb_id = b.lstb_id
        AND a.reg_spaj = b.reg_spaj
        AND a.mspro_prod_ke = b.mspro_prod_ke
        AND a.mspro_prod_date = b.mspro_prod_date
        AND b.lsbs_id =$P{lsbsId}
        AND b.lsdbs_number =$P{lsdbsNumber}
        AND a.reg_spaj = c.reg_spaj
        AND c.lspd_id <> 95
        AND c.reg_spaj = d.reg_spaj
        AND d.no_urut = (select max(no_urut) from eka.mst_reff_bii where level_id = 4 and reg_spaj = d.reg_spaj)
        AND trim(d.lcb_penutup) = trim($P{lcb_no})
        AND c.mspo_policy_holder = e.mcl_id
       $P!{statement1}
    ) b
WHERE a.reg_spaj = b.reg_spaj AND a.lsbs_id > 300
GROUP BY a.lsbs_id, a.lsdbs_number
ORDER BY a.lsbs_id, a.lsdbs_number]]>
	</queryString>
