	<queryString>
		<![CDATA[select lcb_no, nama_cabang, sum(premi_nb_sp) as premi_nb_sp, sum(premi_nb_ssl) as premi_nb_ssl,  sum(premi_nb_nssl) as premi_nb_nssl, sum(premi_nb_simpol) as premi_nb_simpol,
    sum(os_nssl) as os_nssl, sum(os_sp) as os_sp, sum(os_simpol) as os_simpol, sum(os_ssl) as os_ssl,
    sum(polis_in_nssl) as polis_in_nssl, sum(polis_out_nssl) as polis_out_nssl, sum(polis_in_sp) as polis_in_sp, sum(polis_out_sp) as polis_out_sp, 
    sum(polis_in_simpol) as polis_in_simpol, sum(polis_lapse_simpol) as polis_lapse_simpol, sum(polis_ro_ssl) as polis_ro_ssl, sum(polis_out_ssl) as polis_out_ssl
from
(
select e.lcb_no,nama_cabang, 
        sum(case when ((lsbs_id=142 and lsdbs_number= 2) or (lsbs_id = 158 and lsdbs_number=6)) then (b.mspro_nilai_kurs * msdpr_premium) else 0 end ) as premi_nb_sp,
        sum(case when (lsbs_id= 164 and lsdbs_number = 2) then (b.mspro_nilai_kurs * msdpr_premium) else 0 end) as premi_nb_ssl,
 		sum(case when (lsbs_id= 164 and lsdbs_number = 11) then (b.mspro_nilai_kurs * msdpr_premium) else 0 end) as premi_nb_nssl,
        sum(case when lsbs_id = 120 then (b.mspro_nilai_kurs * c.msdpr_premium) else 0 end) premi_nb_simpol,
        0 as os_nssl,
        0 as os_sp,
        0 as os_simpol,
        0 as os_ssl,
        0 as polis_in_nssl,
        0 as polis_out_nssl,
        0 as polis_in_sp,
        0 as polis_out_sp,
        0 as polis_in_simpol,
        0 as polis_lapse_simpol,
        0 as polis_ro_ssl,
        0 as polis_out_ssl
from eka.mst_policy a,
     eka.mst_production b,
     eka.mst_det_production c,
     eka.mst_reff_bii d,
     eka.lst_cabang_bii e
where a.reg_spaj = b.reg_spaj
and a.reg_spaj = c.reg_spaj
and a.reg_spaj = d.reg_spaj
and b.mspro_prod_ke = c.mspro_prod_ke
and b.mspro_prod_date = c.mspro_prod_date
and d.lcb_penutup = e.lcb_no
and d.no_urut = (select max(no_urut) from eka.mst_reff_bii where reg_spaj = a.reg_spaj)
and b.mspro_prod_date = (select min(mspro_prod_date) from eka.mst_production where msbi_tahun_ke = 1 and msbi_premi_ke = 1 and reg_spaj = a.reg_spaj)
and b.mspro_prod_date between  to_date($P{tanggalAwal},'dd/MM/yyyy') and to_date($P{tanggalAkhir},'dd/MM/yyyy')
and jenis = 2
and flag_aktif = 1
group by e.lcb_no, nama_cabang
union
select lcb_no, nama_cabang, 
    0 as premi_nb_sp,
    0 as premi_nb_ssl,
    0 as premi_nb_simpol,
    0 as premi_nb_nssl,
    sum(case when nama_plan = 'STABLE LINK NEW' then (decode(lku_id,'02',9600,1) * saldo_akhir_tb) else 0 end) as os_nssl,
    sum(case when nama_plan = 'POWER SAVE' then (decode(lku_id,'02',9600,1) * saldo_akhir_tb) else 0 end) as os_sp,
    0 as os_simpol,
    sum(case when nama_plan = 'STABLE LINK' then (decode(lku_id,'02',9600,1) * saldo_akhir_tb) else 0 end) as os_ssl,
    sum(case when nama_plan = 'STABLE LINK NEW' then polis_in else 0 end) as polis_in_nssl,
    sum(case when nama_plan = 'STABLE LINK NEW' then polis_out else 0 end) as polis_out_nssl,
    sum(case when nama_plan = 'POWER SAVE' then polis_in else 0 end) as polis_in_sp,
    sum(case when nama_plan = 'POWER SAVE' then polis_out else 0 end) as polis_out_sp,
    0 as polis_in_simpol,
    0 as polis_lapse_simpol,
    (select count(*)
        from eka.mst_powersave_prod
        where lcb_id = lcb_no
        and periode between  to_date($P{tanggalAwal},'dd/MM/yyyy') and to_date($P{tanggalAkhir},'dd/MM/yyyy')) as polis_ro_ssl,
    sum(case when nama_plan = 'STABLE LINK' then polis_out else 0 end) as polis_out_ssl
from eka.mst_powersave_monthly a,
     eka.lst_cabang_bii b
where a.lcb_id = b.lcb_no
and a.periode between  to_date($P{tanggalAwal},'dd/MM/yyyy') and to_date($P{tanggalAkhir},'dd/MM/yyyy')
and saldo_akhir_tb != 0
and jenis = 2
and flag_aktif = 1
group by b.lcb_no, nama_cabang
)
group by lcb_no, nama_cabang
order by nama_cabang]]>
	</queryString>
