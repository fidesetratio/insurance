and d.msps_date between $P{tanggalAwal} AND $P{tanggalAkhir}
and exists (select 1 from eka.mst_product_insured where reg_spaj = a.reg_spaj and lsbs_id >=900)
and a.lstb_id = 1
and a.lca_id in ('37','52')]]>
