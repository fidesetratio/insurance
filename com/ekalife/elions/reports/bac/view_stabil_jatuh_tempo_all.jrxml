	<queryString>
		<![CDATA[SELECT d.nama_cabang, b.mspo_policy_no_format, a.reg_spaj, j.mcl_first pemegang, 
      decode(i.lsbs_id,164,'SIMAS STABIL LINK', '') produk,
      k.lku_symbol, 
      decode(h.msl_ro,1,'Rollover Nilai Tunai',2,'Rollover premi',3,'Autobreak') jenis_ro,
       a.mspro_beg_date, a.mspro_end_date, (h.MSL_EDATE +1) mpr_mature_date, 
       f.nama_reff, f.nama_reff2, l.lus_login_name, a.lku_id,
       nvl(b.mspo_nasabah_dcif,'-') cif, m.nama_cabang cabang_induk, n.nama_cabang wilayah,
       h.msl_mgi, decode(h.msl_no, 1, h.msl_rate , '') msl_rate, decode(h.msl_no, 1, h.msl_premi , 0) msl_premi, decode(h.msl_no, 1, h.msl_bunga , 0) msl_bunga,
       decode(h.msl_no, 1, '', h.msl_mgi) mgi_topup, decode(h.msl_no, 1, '', h.msl_rate) rate_topup, decode(h.msl_no, 1, 0, h.MSL_PREMI) premi_topup, decode(h.msl_no, 1, 0, h.MSL_BUNGA) bunga_topup,
       (decode(h.msl_no, 1, h.msl_premi , 0) + decode(h.msl_no, 1, 0, h.MSL_PREMI)) premi_total,(decode(h.msl_no, 1, h.msl_bunga , 0) + decode(h.msl_no, 1, 0, h.MSL_BUNGA) ) bunga,
       (decode(h.msl_no, 1, h.msl_premi , 0) + decode(h.msl_no, 1, 0, h.MSL_PREMI) + decode(h.msl_no, 1, h.msl_bunga , 0) + decode(h.msl_no, 1, 0, h.MSL_BUNGA) ) hasil_mgi,
       decode(h.flag_bulanan,0,'NON BULANAN','MANFAAT BULANAN') manfaat_bulanan, sim.no_kartu
  FROM eka.mst_production a,
       eka.mst_policy b,
       eka.mst_product_insured c,
       eka.lst_cabang_bii d,
       (select a.reg_spaj ,a.level_id, b.lrb_id, a.lcb_penutup lcb_no, b.nama_reff nama_reff, c.nama_reff nama_reff2
                             from eka.mst_reff_bii a,
                                 eka.lst_reff_bii b,
                                 eka.lst_reff_bii c
                             where      a.level_id=4
                             and a.lrb_id = b.lrb_id
                             and a.reff_id = c.lrb_id
                             and      no_urut=(select max(no_urut) from eka.mst_reff_bii where reg_spaj=a.reg_spaj)) f,
       eka.mst_rek_client g,
       eka.mst_slink h,
       eka.lst_det_bisnis i,
       eka.mst_client j,
       eka.lst_kurs k,
       eka.lst_user l,
       eka.lst_cabang_bii m, 
       eka.lst_cabang_bii n,
       eka.mst_simcard sim
 WHERE c.reg_spaj = b.reg_spaj
   AND a.reg_spaj = c.reg_spaj
   AND g.reg_spaj(+) = b.reg_spaj
   AND h.reg_spaj = b.reg_spaj
   AND h.reg_spaj IN (
          SELECT reg_spaj
            FROM eka.mst_slink
          MINUS
          (SELECT reg_spaj
             FROM eka.mst_pinjaman
           UNION
           SELECT reg_spaj
             FROM eka.mst_slink
            WHERE msl_kode = 4))
   AND f.reg_spaj = b.reg_spaj
   AND f.lcb_no = d.lcb_no
   AND c.lsbs_id = i.lsbs_id
   AND c.lsdbs_number = i.lsdbs_number
   AND j.mcl_id = b.mspo_policy_holder
   AND a.reg_spaj = sim.reg_spaj(+)
   AND (trunc((h.MSL_EDATE +1)) BETWEEN $P{tanggalAwal} and $P{tanggalAkhir})
   AND c.lsbs_id IN (164)
   AND d.jenis = $P{jn_bank}
   AND b.lca_id = '09'
   AND a.msbi_premi_ke = 1
   AND f.level_id = 4
   AND h.msl_kode IN (2, 3, 5)
   AND b.lku_id = k.lku_id
   AND b.lus_id = l.lus_id
   AND d.head_no = m.lcb_no(+)
   AND d.wil_no = n.lcb_no(+)
   ORDER BY nama_cabang, mspo_policy_no_format, h.msl_no]]>
	</queryString>
