	<queryString>
		<![CDATA[SELECT d.nama_cabang, b.mspo_policy_no_format, a.reg_spaj, j.mcl_first pemegang, 
	  decode(i.lsbs_id,164,'SIMAS STABIL LINK', '') produk,
	  k.lku_symbol, i.lsdbs_name,
	  decode(h.msl_ro,1,'Rollover Nilai Tunai',2,'Rollover premi',3,'Autobreak') jenis_ro,
	   a.mspro_beg_date, a.mspro_end_date, (h.MSL_EDATE +1) mpr_mature_date, 
       e.nama_reff, l.lus_login_name, a.lku_id,
	   nvl(b.mspo_nasabah_dcif,'-') cif, nvl(m.nama_cabang,'-') cabang_induk, nvl(n.nama_cabang,'-') wilayah,
	   h.msl_mgi, nvl(h.msl_rate,0) msl_rate, nvl(h.msl_premi,0) msl_premi, nvl(h.msl_bunga,0) msl_bunga,
       o.msl_mgi mgi_topup, nvl(o.msl_rate,0) rate_topup, nvl(o.MSL_PREMI,0) premi_topup, nvl(o.MSL_BUNGA,0) bunga_topup,
	   (nvl(o.MSL_PREMI,0)+nvl(h.MSL_PREMI,0)) premi_total,(nvl(o.MSL_BUNGA,0)+nvl(h.MSL_BUNGA,0)) bunga,
       (nvl(o.MSL_PREMI,0) + nvl(o.MSL_BUNGA,0)+nvl(h.MSL_PREMI,0) + nvl(h.MSL_BUNGA,0)) hasil_mgi
  FROM eka.mst_production a,
       eka.mst_policy b,
       (select * from eka.mst_product_insured where lsbs_id in (164,174) ) c,
       (SELECT a.reg_spaj, nvl(nvl(b.nama_cabang, c.nama_cabang),'-') nama_cabang, b.lrb_id, b.lcb_no, b.head_no, b.wil_no, b.level_id
		  FROM eka.mst_policy a,
		       (SELECT a.reg_spaj, a.lrb_id, a.lcb_no, b.nama_cabang, b.head_no, b.wil_no, a.level_id
		          FROM (select * 
 							from eka.mst_reff_bii a
							 where	  level_id=4
 							and	  no_urut=(select max(no_urut) from eka.mst_reff_bii where reg_spaj=a.reg_spaj)) a, 
							eka.lst_cabang_bii b
		         WHERE a.lcb_no = b.lcb_no AND a.level_id = 4) b,
		       (SELECT b.mns_kd_nasabah, c.nama_cabang
		          FROM eka.mst_nasabah b, eka.lst_cab_bii c
		         WHERE b.kd_region = c.kd_region
		           AND b.kd_koord = c.kd_koord
		           AND b.kd_area = c.kd_area
		           AND b.kd_cabang = c.kd_cabang) c
		 WHERE a.reg_spaj = b.reg_spaj(+) AND a.mspo_plan_provider = c.mns_kd_nasabah(+)) d,
       eka.lst_reff_bii e,
       eka.mst_rek_client g,
       eka.mst_slink h,
       eka.lst_det_bisnis i,
       eka.mst_client j,
       eka.lst_kurs k,
	   eka.lst_user l,
	   eka.lst_cabang_bii m, 
	   eka.lst_cabang_bii n,
	   (select * from eka.mst_slink n where n.MSL_NO =2  AND n.msl_kode IN (2, 3, 5))o
 WHERE c.reg_spaj = b.reg_spaj
   AND a.reg_spaj = c.reg_spaj
   AND g.reg_spaj(+) = b.reg_spaj
   AND h.reg_spaj = b.reg_spaj
   AND h.reg_spaj IN (
          SELECT reg_spaj
            FROM eka.mst_slink
          MINUS
          (SELECT reg_spaj
             FROM eka.mst_pinjaman
           UNION
           SELECT reg_spaj
             FROM eka.mst_slink
            WHERE msl_kode = 4))
   AND o.reg_spaj IN (
          SELECT reg_spaj
            FROM eka.mst_slink
          MINUS
          (SELECT reg_spaj
             FROM eka.mst_pinjaman
           UNION
           SELECT reg_spaj
             FROM eka.mst_slink
            WHERE msl_kode = 4))	
   AND d.reg_spaj = b.reg_spaj
   AND d.lrb_id = e.lrb_id(+)
   AND c.lsbs_id = i.lsbs_id
   AND c.lsdbs_number = i.lsdbs_number
   AND j.mcl_id = b.mspo_policy_holder
   AND (trunc((h.MSL_EDATE +1)) BETWEEN $P{tanggalAwal} and $P{tanggalAkhir})
   AND a.msbi_premi_ke = 1
   AND h.msl_kode IN (2, 3, 5)
   AND b.reg_spaj = o.reg_spaj (+)
   AND h.msl_no=1
   AND b.lku_id = k.lku_id
   AND b.lus_id = l.lus_id
   AND d.head_no = m.lcb_no(+)
   AND d.wil_no = n.lcb_no(+)
   ORDER BY d.nama_cabang, i.lsdbs_name, b.mspo_policy_no_format, h.msl_edate]]>
	</queryString>
