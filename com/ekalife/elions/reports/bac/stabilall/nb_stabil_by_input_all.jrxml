	<queryString>
		<![CDATA[SELECT * FROM (
SELECT b.lku_id, k.lku_symbol, nvl(b.mspo_nasabah_dcif,'-') mspo_nasabah_dcif, f.mcl_first pemegang, g.mcl_first tertanggung,
       b.mspo_policy_no_format, b.reg_spaj, a.lus_login_name, e.nama_cabang,
       h.lsbs_name, 
       (case when i.lsbs_id in (142,158) then 'SIMAS PRIMA'
				 when i.lsbs_id = 175 then 'SIMAS PRIMA SYARIAH'
				 else i.lsdbs_name end) lsdbs_name, 
       d.mspr_premium, d.mspr_tsi,
         j.msl_mgi, nvl(j.msl_rate,0) msl_rate, nvl(j.msl_premi,0) msl_premi, nvl(j.msl_bunga,0) msl_bunga,
       n.msl_mgi mgi_topup, nvl(n.msl_rate,0) rate_topup, nvl(n.MSL_PREMI,0) premi_topup, nvl(n.MSL_BUNGA,0) bunga_topup,
	   (nvl(n.MSL_PREMI,0)+nvl(j.MSL_PREMI,0)) premi_total,(nvl(n.MSL_BUNGA,0)+nvl(j.MSL_BUNGA,0)) bunga,
       (nvl(n.MSL_PREMI,0) + nvl(n.MSL_BUNGA,0)+nvl(j.MSL_PREMI,0) + nvl(j.MSL_BUNGA,0)) hasil_mgi,
	   b.mspo_ins_period,
       b.mspo_date_print, d.mspr_beg_date, c.mste_tgl_aksep, nvl(l.nama_cabang,'-') cabang_induk, nvl(m.nama_cabang,'-') wilayah
  FROM eka.lst_user a,
       eka.mst_policy b,
       eka.mst_insured c,
       (select * from eka.mst_product_insured where lsbs_id in (164,174) ) d,
       (SELECT a.reg_spaj, nvl(nvl(b.nama_cabang, c.nama_cabang),'-') nama_cabang, b.lrb_id, b.lcb_no, b.head_no, b.wil_no
		  FROM eka.mst_policy a,
		       (SELECT a.reg_spaj, a.lrb_id, a.lcb_no, b.nama_cabang, b.head_no, b.wil_no
		          FROM (select * 
 							from eka.mst_reff_bii a
							 where	  level_id=4
 							and	  no_urut=(select max(no_urut) from eka.mst_reff_bii where reg_spaj=a.reg_spaj)) a, 
							eka.lst_cabang_bii b
		         WHERE a.lcb_no = b.lcb_no AND a.level_id = 4) b,
		       (SELECT b.mns_kd_nasabah, c.nama_cabang
		          FROM eka.mst_nasabah b, eka.lst_cab_bii c
		         WHERE b.kd_region = c.kd_region
		           AND b.kd_koord = c.kd_koord
		           AND b.kd_area = c.kd_area
		           AND b.kd_cabang = c.kd_cabang) c
		 WHERE a.reg_spaj = b.reg_spaj(+) AND a.mspo_plan_provider = c.mns_kd_nasabah(+)) e,
       eka.mst_client_new f,
       eka.mst_client_new g,
       eka.lst_bisnis h,
       eka.lst_det_bisnis i,
       eka.mst_slink j,
	   eka.lst_kurs k,
	   eka.lst_cabang_bii l, 
	   eka.lst_cabang_bii m,
       (select * from eka.mst_slink n where n.MSL_NO =2  AND n.msl_kode = 5)n
 WHERE a.lus_id = b.lus_id
   AND b.lspd_id <> 95
   AND b.reg_spaj = c.reg_spaj
   AND b.reg_spaj = d.reg_spaj
   AND d.lsbs_id < 300
   AND b.reg_spaj = e.reg_spaj
   AND b.mspo_policy_holder = f.mcl_id
   AND c.mste_insured = g.mcl_id
   AND d.lsbs_id = h.lsbs_id
   AND d.lsbs_id = i.lsbs_id
   AND d.lsdbs_number = i.lsdbs_number
   AND b.reg_spaj = j.reg_spaj
   AND j.msl_kode = 5
   AND b.reg_spaj = n.reg_spaj(+)
   AND j.msl_no=1
   AND b.lku_id = k.lku_id
   AND e.head_no = l.lcb_no(+)
   AND e.wil_no = m.lcb_no(+)
   AND trunc(b.mspo_input_date) between  $P{tanggalAwal} and $P{tanggalAkhir}
) ORDER BY NAMA_CABANG, LSDBS_NAME, MSPO_POLICY_NO_FORMAT, MSPO_DATE_PRINT]]>
