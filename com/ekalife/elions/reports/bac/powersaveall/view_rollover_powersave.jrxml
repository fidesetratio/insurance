	<queryString>
		<![CDATA[SELECT e.nama_cabang, b.mspo_policy_no_format, a.reg_spaj, l.mcl_first pemegang,   
	   i.lsdbs_name produk,
	   k.mpr_jangka_invest MGI, m.lku_symbol,
	   c.mspr_premium, decode(k.mpr_jns_ro,1,'Rollover Nilai Tunai',2,'Rollover premi',3,'Autobreak') jenis_ro, 
	   a.mspro_beg_date, a.mspro_end_date, k.mpr_mature_date, 
	   h.nama_reff, n.lus_login_name, g.lku_id,
	   nvl(b.mspo_nasabah_dcif,'-') cif, o.nama_cabang cabang_induk, nvl(p.nama_cabang,'-') wilayah
  FROM eka.mst_production a,
       eka.mst_policy b,
       eka.mst_product_insured c,
       (SELECT a.reg_spaj, nvl(nvl(b.nama_cabang, c.nama_cabang),'-') nama_cabang, b.lrb_id, b.lcb_no, b.head_no, b.wil_no, b.level_id
		  FROM eka.mst_policy a,
		       (SELECT a.reg_spaj, a.lrb_id, a.lcb_no, b.nama_cabang, b.head_no, b.wil_no, a.level_id
		          FROM (select * 
 							from eka.mst_reff_bii a
							 where	  level_id=4
 							and	  no_urut=(select max(no_urut) from eka.mst_reff_bii where reg_spaj=a.reg_spaj)) a, 
							eka.lst_cabang_bii b
		         WHERE a.lcb_no = b.lcb_no AND a.level_id = 4) b,
		       (SELECT b.mns_kd_nasabah, c.nama_cabang
		          FROM eka.mst_nasabah b, eka.lst_cab_bii c
		         WHERE b.kd_region = c.kd_region
		           AND b.kd_koord = c.kd_koord
		           AND b.kd_area = c.kd_area
		           AND b.kd_cabang = c.kd_cabang) c
		 WHERE a.reg_spaj = b.reg_spaj(+) AND a.mspo_plan_provider = c.mns_kd_nasabah(+)) e,
       eka.mst_rek_client f,
       eka.mst_billing g,
       eka.lst_reff_bii h,
       eka.lst_det_bisnis i,
       eka.mst_powersave_ro k,
       eka.mst_client_new l,
	   eka.lst_kurs m,
	   eka.lst_user n,
	   eka.lst_cabang_bii o, 
	   eka.lst_cabang_bii p
 WHERE c.reg_spaj = b.reg_spaj
   AND b.mspo_policy_holder = l.mcl_id
   AND a.reg_spaj = c.reg_spaj
   AND g.reg_spaj = a.reg_spaj
   AND g.msbi_tahun_ke = a.msbi_tahun_ke
   AND g.msbi_premi_ke = a.msbi_premi_ke
   AND f.reg_spaj(+) = b.reg_spaj
   AND k.reg_spaj = b.reg_spaj
   AND e.reg_spaj = b.reg_spaj
   AND h.lrb_id = e.lrb_id
   AND e.lcb_no = h.lcb_no
   AND i.lsbs_id = c.lsbs_id
   AND i.lsdbs_number = c.lsdbs_number
   AND b.lku_id = m.lku_id
   AND (trunc(k.mpr_tgl_prod) BETWEEN $P{tanggalAwal} and $P{tanggalAkhir})
   AND e.level_id = 4
   AND b.lus_id = n.lus_id
   AND k.reg_spaj IN (
                SELECT reg_spaj
                  FROM eka.mst_powersave_ro
                MINUS
                SELECT reg_spaj
                  FROM eka.mst_pinjaman
                 WHERE TRUNC (mspin_tgl_input) BETWEEN $P{tanggalAwal} and $P{tanggalAkhir})
   AND c.lsbs_id in (142,143,144,158,175)
   AND a.msbi_tahun_ke = 1
   AND k.mps_kode IN (2, 3)
   AND e.head_no = o.lcb_no(+)
   AND e.wil_no = p.lcb_no(+)
   ORDER BY e.nama_cabang, i.lsdbs_name, b.mspo_policy_no, n.lus_login_name, k.mpr_mature_date]]>
	</queryString>
