	<queryString>
		<![CDATA[SELECT /*+ first_rows */
		 DISTINCT EKA.MST_POLICY.MSPO_POLICY_NO,
		 EKA.MST_POLICY.MSPO_POLICY_NO_FORMAT,   
         EKA.MST_CLIENT.MCL_FIRST,   
         EKA.MST_POLICY.LCA_ID,
		 EKA.MST_POLICY.LWK_ID,
		 EKA.MST_POLICY.LKU_ID,   
         EKA.MST_POLICY.LSSP_ID,
		 EKA.LST_CABANG.LCA_NAMA,
		 EKA.LST_WAKIL.LWK_NAMA,
		 EKA.MST_SSAVE.MSS_KODE,   
         EKA.MST_SSAVE.MSS_BDATE,   
         EKA.MST_SSAVE.MSS_EDATE,   
         EKA.MST_SSAVE.MSS_MGI,   
         EKA.MST_SSAVE.MSS_RATE,   
         EKA.MST_SSAVE.MSS_PREMI,   
         EKA.MST_SSAVE.MSS_BUNGA,   
         EKA.MST_SSAVE.MSS_TAX,   
         EKA.MST_SSAVE.MSS_NOTE,   
         EKA.MST_SSAVE.MSS_RO,   
         EKA.MST_SSAVE.MSS_FLAG_COMM,   
         EKA.MST_SSAVE.REG_SPAJ,   
         EKA.MST_SSAVE.MSS_FLAG_RATE,
		 EKA.MST_SSAVE.MSS_INPUT_DATE,
		 EKA.MST_SSAVE.FLAG_BULANAN,
		 EKA.MST_PRODUCT_INSURED.LSBS_ID,
		 EKA.MST_PRODUCT_INSURED.LSDBS_NUMBER,
		 EKA.MST_PRODUCT_INSURED.MSPR_TSI,
		 EKA.MST_NASABAH.KODE,
		 '' AS flag_pinjaman,
		 (CASE WHEN EKA.MST_SSAVE.MSS_RO = 1 THEN 'ROLLOVER NILAI TUNAI'
					WHEN EKA.MST_SSAVE.MSS_RO = 2 THEN 'ROLLOVER PREMI'
					WHEN EKA.MST_SSAVE.MSS_RO = 3 THEN 'AUTOBREAK'
					ELSE 'NONE' END) JENIS_RO,
		 EKA.LST_DET_BISNIS.LSDBS_NAME,
		 EKA.LST_REFERRER_BII.NAMA_REF,
		 EKA.MST_AGENT.MSAG_ID AGENT_CODE,
		 EKA.MST_CLIENT_NEW.MCL_FIRST AGENT_NAME,
		 (CASE WHEN EKA.MST_POLICY.LCA_ID IN('37','42','46','52') THEN EKA.LST_WAKIL.LWK_NAMA
					ELSE EKA.LST_CABANG.LCA_NAMA END) NAMA_CABANG,
		 EKA.LST_KURS.LKU_SYMBOL
    FROM EKA.MST_SSAVE,
			  EKA.MST_CLIENT,
       		  EKA.LST_POLICY_STATUS,    
        	  EKA.MST_POLICY,   
         	  EKA.LST_CABANG,
			  EKA.LST_WAKIL,
			  EKA.MST_PRODUCT_INSURED,
			  EKA.LST_DET_BISNIS,
  		      EKA.MST_NASABAH ,
			  EKA.LST_REFERRER_BII,
			  EKA.MST_AGENT_PROD,
			  EKA.MST_AGENT,
			  EKA.MST_CLIENT_NEW,
			  EKA.LST_KURS
   WHERE ( EKA.MST_SSAVE.REG_SPAJ		 	 	 	 = EKA.MST_POLICY.REG_SPAJ 						) AND  
         ( EKA.MST_POLICY.MSPO_POLICY_HOLDER 	 = EKA.MST_CLIENT.MCL_ID 							) AND
         ( EKA.MST_POLICY.LSSP_ID		       	 = EKA.LST_POLICY_STATUS.LSSP_ID 				) AND  
			( EKA.MST_POLICY.LCA_ID                 = EKA.LST_CABANG.LCA_ID                     ) AND
			( EKA.MST_POLICY.LCA_ID                 = EKA.LST_WAKIL.LCA_ID                      ) AND
			( EKA.MST_POLICY.LWK_ID                 = EKA.LST_WAKIL.LWK_ID                      ) AND
			( EKA.MST_POLICY.REG_SPAJ					 = EKA.MST_PRODUCT_INSURED.REG_SPAJ				) AND
			( EKA.MST_PRODUCT_INSURED.LSBS_ID		 = EKA.LST_DET_BISNIS.LSBS_ID						) AND
			( EKA.MST_PRODUCT_INSURED.LSDBS_NUMBER	 = EKA.LST_DET_BISNIS.LSDBS_NUMBER				) AND
			( EKA.MST_NASABAH.MNS_KD_NASABAH   (+)	 = EKA.MST_POLICY.MSPO_PLAN_PROVIDER 			) AND
			( EKA.LST_REFERRER_BII.REFERRER_ID (+)	 = EKA.MST_NASABAH.REFERRER_ID 					) AND
			( EKA.MST_POLICY.REG_SPAJ               = EKA.MST_AGENT_PROD.REG_SPAJ               ) AND
			( EKA.MST_AGENT_PROD.MSAG_ID            = EKA.MST_AGENT.MSAG_ID                     ) AND
			( EKA.MST_AGENT.MCL_ID                  = EKA.MST_CLIENT_NEW.MCL_ID                 ) AND
			(EKA.MST_POLICY.LKU_ID					 = EKA.LST_KURS.LKU_ID) AND
 			( ( EKA.MST_PRODUCT_INSURED.LSBS_ID		 <= 300														 )   AND
            ( EKA.MST_SSAVE.MSS_KODE         			IN (2,3,5) 												 )   AND
		    ( EKA.MST_POLICY.LUS_ID				= $P{lus_id}									) AND  
            ( EKA.MST_SSAVE.MSS_EDATE  					BETWEEN to_date($P{startDate},'dd/mm/yyyy') and to_date($P{endDate},'dd/mm/yyyy') 					 )   AND  
            ( EKA.LST_POLICY_STATUS.LMS_ID 			 = 1			 												 )   AND
			  ( EKA.MST_AGENT_PROD.LSLE_ID            = 4                                             )   AND
		     ( NOT EXISTS (
		  	                SELECT 1
					          FROM EKA.MST_PINJAMAN
					          WHERE EKA.MST_PINJAMAN.REG_SPAJ = EKA.MST_POLICY.REG_SPAJ AND
					  	             EKA.MST_PINJAMAN.FLAG_PS  = 3
 		 	 		         )                                                                              ) )]]>
	</queryString>
