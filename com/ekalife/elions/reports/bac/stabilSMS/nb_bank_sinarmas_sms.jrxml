	<queryString>
		<![CDATA[SELECT 
	   sim.no_kartu, b.mspo_input_date,b.lku_id, b.reg_spaj, d.mcl_first pemegang, c.mcl_first tertanggung, n.lus_login_name,
	   b.mspo_policy_no_format, k.lku_symbol, e.mspr_tsi, 
	   l.msl_mgi msl_mgi, decode(l.msl_no, 1, l.msl_rate , '') msl_rate, decode(l.msl_no, 1, l.msl_premi , 0) premi, decode(l.msl_no, 1, l.msl_bunga , 0) bunga,
       decode(l.msl_no, 1, '', l.msl_mgi) mgi_topup, decode(l.msl_no, 1, '', l.msl_rate) rate_topup, decode(l.msl_no, 1, 0, l.MSL_PREMI) premi_topup, decode(l.msl_no, 1, 0, l.MSL_BUNGA) bunga_total,
	   (decode(l.msl_no, 1, l.msl_premi , 0) + decode(l.msl_no, 1, 0, l.MSL_PREMI)) premi_total,(decode(l.msl_no, 1, l.msl_bunga , 0) + decode(l.msl_no, 1, 0, l.MSL_BUNGA) ) bunga,
       (decode(l.msl_no, 1, l.msl_premi , 0) + decode(l.msl_no, 1, 0, l.MSL_PREMI) + decode(l.msl_no, 1, l.msl_bunga , 0) + decode(l.msl_no, 1, 0, l.MSL_BUNGA) ) hasil_mgi,
	   NVL (b.mspo_nasabah_dcif, '-') mspo_nasabah_dcif,       
       o.lsdbs_name, a.mste_tgl_aksep, b.mspo_date_print, g.nama_reff,
       h.nama_cabang, i.nama_cabang cabang_induk, j.nama_cabang wilayah, n.LUS_LOGIN_NAME,  e.mspr_beg_date,
	   decode(l.flag_bulanan,0,'NON BULANAN','MANFAAT BULANAN') manfaat_bulanan
  FROM eka.mst_insured a, 
  	   eka.mst_policy b, 
	   eka.mst_client_new c, 
	   eka.mst_client_new d,   
	   eka.mst_product_insured e,
  	   (select a.reg_spaj ,a.lcb_penutup lcb_no,b.lrb_id 
 							from eka.mst_reff_bii a,
								 eka.lst_reff_bii b
							 where	  a.level_id=4
							 and a.lrb_id = b.lrb_id
 							and	  no_urut=(select max(no_urut) from eka.mst_reff_bii where reg_spaj=a.reg_spaj)) f,
	   eka.lst_reff_bii g,   
	   eka.lst_cabang_bii h,
	   eka.lst_cabang_bii i,
	   eka.lst_cabang_bii j,
	   eka.lst_kurs k, 
	   eka.mst_slink l, 
	   eka.lst_user n,  
	   eka.lst_det_bisnis o,
	   eka.mst_simcard sim
  WHERE a.reg_spaj=b.reg_spaj
   AND b.lspd_id <> 95
   AND a.MSTE_INSURED =c.MCL_ID
   AND b.MSPO_POLICY_HOLDER=d.mcl_id
   AND e.REG_SPAJ=a.REG_SPAJ
   AND e.LSBS_ID = 164
   AND e.LSDBS_NUMBER =8
   AND a.MSTE_INSURED_NO=1
   AND b.reg_spaj = f.reg_spaj
   AND f.lrb_id = g.lrb_id
   AND f.lcb_no = h.lcb_no
   AND b.lku_id = k.lku_id
   AND h.head_no = i.lcb_no(+)
   AND h.wil_no = j.lcb_no(+)
   AND a.reg_spaj=l.reg_spaj
   AND l.msl_kode = 5
   AND l.msl_new = 1
   and b.LUS_ID=n.LUS_ID
   AND e.lsbs_id = o.lsbs_id
   AND e.lsdbs_number = o.lsdbs_number
   AND b.reg_spaj = sim.reg_spaj(+)
   AND   (a.MSTE_TGL_AKSEP BETWEEN $P{tanggalAwal} and $P{tanggalAkhir}
       )
ORDER BY i.nama_cabang, j.nama_cabang, h.NAMA_CABANG, n.LUS_LOGIN_NAME, a.mste_tgl_aksep, l.msl_no]]>
