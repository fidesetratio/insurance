	<queryString>
		<![CDATA[SELECT   mpm.periode, mpm.tahun, mpm.bulan, nvl(wilayah.nama_cabang, 'Lainnya') wilayah,
         nvl(cabang_induk.nama_cabang, ' ') cabang_induk, cabang.nama_cabang cabang,
         mpm.nama_plan, mpm.lku_id, lk.lku_symbol, mpm.lsbp_nama, mpm.saldo_awal,
         mpm.masuk, mpm.deposit, mpm.bunga, mpm.saldo_akhir
    FROM eka.mst_powersave_monthly mpm,
         eka.lst_cabang_bii cabang,
         eka.lst_cabang_bii cabang_induk,
         eka.lst_cabang_bii wilayah,
         eka.lst_kurs lk
   WHERE mpm.lsbp_id = 156
     AND mpm.periode BETWEEN $P{tanggalAwal} and $P{tanggalAkhir}
     AND mpm.lcb_id = cabang.lcb_no
     AND mpm.lku_id = lk.lku_id
	 AND cabang.jenis = 2
	AND substr(mpm.lcb_id, 1, 1) = 'S'
     AND cabang.head_no = cabang_induk.lcb_no(+)
     AND cabang.wil_no = wilayah.lcb_no(+)
ORDER BY mpm.periode,
         wilayah.nama_cabang,
         cabang_induk.nama_cabang,
         cabang.nama_cabang,
         mpm.nama_plan,
         mpm.lku_id]]>
