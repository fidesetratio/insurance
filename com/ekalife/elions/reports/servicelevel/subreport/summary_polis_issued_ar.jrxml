	<queryString>
		<![CDATA[SELECT   b.reg_spaj, d.lsdbs_name, a.mste_tgl_terima_spaj,
         a.mste_tgl_spaj_doc, a.mste_tgl_aksep, b.mspo_date_print,
         a.mste_tgl_kirim_polis, e.lsrg_nama,
			nvl((SELECT eka.workdays (
				(CASE WHEN a.mste_tgl_spaj_doc is null THEN a.mste_tgl_spaj_doc WHEN a.mste_tgl_spaj_doc >= to_date(to_char(mste_tgl_spaj_doc,'dd/mm/yyyy')||' 12:00:00','dd/mm/yyyy hh24:mi:ss') THEN a.mste_tgl_spaj_doc+1 ELSE a.mste_tgl_spaj_doc END), 
				(CASE WHEN h.jn_bank=2 THEN a.mste_tgl_aksep ELSE a.mste_tgl_kirim_polis END)) 
			FROM DUAL),0) lama_proses_nab,
         nvl((SELECT eka.workdays (b.mspo_next_bill, (CASE WHEN h.jn_bank=2 THEN a.mste_tgl_aksep ELSE nvl(a.mste_tgl_valid_print, a.mste_tgl_kirim_polis) END)) FROM DUAL), 0) lama_proses_nab1,
         nvl((SELECT eka.workdays ((CASE WHEN a.mste_tgl_spaj_doc is null THEN a.mste_tgl_spaj_doc WHEN a.mste_tgl_spaj_doc >= to_date(to_char(mste_tgl_spaj_doc,'dd/mm/yyyy')||' 12:00:00','dd/mm/yyyy hh24:mi:ss') THEN a.mste_tgl_spaj_doc+1 ELSE a.mste_tgl_spaj_doc END), b.mspo_next_bill) FROM DUAL), 0) lama_proses_nab2,
         nvl((SELECT eka.workdays ((CASE WHEN h.jn_bank=2 THEN a.mste_tgl_aksep ELSE nvl(a.mste_tgl_valid_print, a.mste_tgl_kirim_polis) END), b.mspo_next_bill) FROM DUAL), 0) lama_proses_nab2,
         nvl((SELECT eka.workdays (a.mste_tgl_terima_spaj, SYSDATE) FROM DUAL), 0) lama_pending, 
		 SYSDATE, d.lsbs_id, d.lsdbs_number, b.lca_id, DECODE (b.lca_id, '42', 1, 0) flag_ws
    FROM eka.mst_policy b,
         eka.mst_insured a,
         eka.mst_product_insured c,
         eka.lst_det_bisnis d,
         eka.lst_region e,
eka.lst_bisnis g,
eka.lst_user h
   WHERE b.reg_spaj = a.reg_spaj
and b.lca_id not in ('09','08','42')
     AND b.lstb_id = 1
     AND (a.mste_tgl_aksep BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir})
     AND a.mste_tgl_kirim_polis IS NOT NULL
     AND b.reg_spaj = c.reg_spaj
     AND c.lsbs_id < 300
     AND c.lsbs_id = d.lsbs_id
     AND c.lsdbs_number = d.lsdbs_number
     AND b.lca_id = e.lca_id
     AND b.lwk_id = e.lwk_id
     AND b.lsrg_id = e.lsrg_id
	AND c.lsbs_id = g.lsbs_id
and b.lus_id = h.lus_id
ORDER BY a.mste_tgl_terima_spaj, b.reg_spaj]]>
