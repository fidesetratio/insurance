	<queryString>
		<![CDATA[SELECT NVL (j.mscan_tgl_batal, b.mspo_spaj_date) mspo_spaj_date,
       d.mss_mgi mps_jangka_inv, d.mss_bunga mps_prm_interest, b.reg_spaj, b.lku_id,
       e.lku_symbol, b.lscb_id, b.mspo_policy_no, b.mspo_no_blanko,
       b.mspo_date_print, b.mspo_date_spec, b.lsre_id, a.mste_age,
       a.mste_beg_date, a.mste_end_date,
       eka.pemegang(pp.mcl_id,(select upper(lti_note || ' ') from eka.lst_title where lti_id = pp.lti_id) || pp.mcl_first, b.mspo_age, pp.mcl_jenis, 1) nama_pp, tt.mcl_gelar,
       tt.mspe_date_birth, c.lsbs_id, c.lsdbs_number, c.mspr_tsi,
       c.mspr_premium,
       (SELECT UPPER (lti_note || ' ')
          FROM eka.lst_title
         WHERE lti_id = tt.lti_id) || tt.mcl_first nama_tt, d.mss_rate mps_rate,
       DECODE (d.mss_flag_rate,
               1, '(Special-Gross)',
               2, '(Special-Nett)',
               ' '
              ) jenis_bunga,
       (case when k.lsbs_id =177 then 'PROGRESSIVE SAVE' 
		else k.lsdbs_name end) judul, d.flag_bulanan, l.lscb_pay_mode, b.mspo_pay_period
  FROM eka.mst_insured a,
       eka.mst_policy b,
       eka.mst_product_insured c,
       eka.mst_client_new pp,
       eka.mst_client_new tt,
       eka.mst_ssave d,
       eka.lst_kurs e,
       (SELECT reg_spaj, mscan_spaj_baru, mscan_tgl_batal
          FROM eka.mst_cancel
         WHERE mscan_spaj_baru = RPAD ($P{spaj}, 11, ' ')) j,
       eka.lst_det_bisnis k,
	   eka.lst_pay_mode l
 WHERE a.reg_spaj = b.reg_spaj
   AND c.reg_spaj = a.reg_spaj
   AND b.lscb_id = l.lscb_id
   AND b.reg_spaj = j.mscan_spaj_baru(+)
   AND c.mste_insured_no = a.mste_insured_no
   AND a.mste_insured = tt.mcl_id(+)
   AND b.mspo_policy_holder = pp.mcl_id(+)
   AND d.reg_spaj = b.reg_spaj
   AND b.reg_spaj = RPAD ($P{spaj}, 11, ' ')
   AND c.lsbs_id < 300
   AND b.lku_id = e.lku_id
   AND d.mss_kode = 5
   AND c.lsbs_id = k.lsbs_id
   AND c.lsdbs_number = k.lsdbs_number]]>
	</queryString>
