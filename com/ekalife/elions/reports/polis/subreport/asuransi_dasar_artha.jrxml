	<queryString>
		<![CDATA[SELECT a.reg_spaj, a.lscb_id, b.mspr_premium, e.lku_symbol, b.mspr_tsi,
       c.mste_beg_date, c.mste_end_date, b.mspr_end_pay,
      (case when (b.lsbs_id=162 and b.lsdbs_number in (5,6)) then 'EKALINK 88'
                        when (b.lsbs_id=162 and b.lsdbs_number in (7,8)) then 'EKALINK 88 PLUS'
					    when b.lsbs_id =138 then 'SMiLe LINK 88 KRY'
                        when b.lsbs_id=199 then 'MEDIVEST SYARIAH' 
				when (b.lsbs_id = 190 and b.lsdbs_number = 4) then 'SMiLe LINK 99 REGULER'
                when (b.lsbs_id = 190 and b.lsdbs_number = 3) then 'SMiLe LINK 99 SINGLE'
					when (b.lsbs_id = 190 and b.lsdbs_number in( 5,6)) then 'VIP Family Plan'
 					when (b.lsbs_id = 190 and b.lsdbs_number in( 7)) then 'SMiLe OPTIMA LINK SEKALIGUS'
				      when (b.lsbs_id = 217 and b.lsdbs_number in( 2)) then 'SMiLe LINK PRO 100' 
                      else eka.f_lsdbs(a.reg_spaj, d.lsbs_id,f.lsdbs_number) end)
                      lsbs_judul1,
       (case when c.mste_beg_date < to_date('01/12/2012','dd/MM/yyyy') then d.lsbs_judul2
             else d.group_name
       end)
       lsbs_judul2,
       nvl(biasu.mbu_end_pay, b.mspr_end_pay) akhir_biaya_asuransi, 
        nvl(biamin.mbu_end_pay, (add_months(c.mste_end_date,-1)+1)) akhir_biaya_admin,
        f.lsbs_id, f.lsdbs_number
  FROM eka.mst_policy a,
       eka.mst_product_insured b,
       eka.mst_insured c,
       eka.lst_bisnis d,
       eka.lst_kurs e,
       eka.lst_det_bisnis f,
       (select * from eka.mst_biaya_ulink where ljb_id = 2 and reg_spaj = rpad($P{spaj},11,' ')) biasu,
       (select * from eka.mst_biaya_ulink where ljb_id = 3 and reg_spaj = rpad($P{spaj},11,' ')) biamin
 WHERE a.reg_spaj = b.reg_spaj
   AND b.reg_spaj = c.reg_spaj
   AND b.lsbs_id = d.lsbs_id
   AND a.reg_spaj = rpad($P{spaj},11,' ')
   AND b.lsbs_id < 300
   AND b.lku_id = e.lku_id
   AND a.reg_spaj = biasu.reg_spaj
   AND a.reg_spaj = biamin.reg_spaj
   AND b.lsbs_id = f.lsbs_id
   AND b.lsdbs_number = f.lsdbs_number]]>
	</queryString>
