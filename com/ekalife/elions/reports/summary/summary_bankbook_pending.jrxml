  FROM eka.mst_dbank, eka.mst_tbank, eka.lst_user, eka.mst_bvoucher
 WHERE eka.mst_tbank.no_pre = eka.mst_dbank.no_pre
   AND eka.mst_tbank.lus_id = eka.lst_user.lus_id
   AND eka.mst_dbank.no_pre = eka.mst_bvoucher.no_pre
   AND eka.mst_dbank.no_jurnal = eka.mst_bvoucher.no_jurnal
   AND TRUNC (eka.mst_tbank.tgl_trans_pending) >= $P{tglAwal}
   AND TRUNC (eka.mst_tbank.tgl_trans_pending) <= $P{tglAkhir}
   AND eka.mst_tbank.lus_id = $P{lus_id}
   AND eka.mst_tbank.tgl_trans_pending is not null
   AND eka.mst_tbank.POSITION = 3
