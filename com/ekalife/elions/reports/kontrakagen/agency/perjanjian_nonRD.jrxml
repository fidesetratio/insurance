				<reportElement x="3" y="2317" width="11" height="28"/>
				<textElement/>
				<text><![CDATA[2.]]></text>
			</staticText>
			<staticText>
				<reportElement x="15" y="2250" width="11" height="14"/>
				<textElement/>
				<text><![CDATA[b.]]></text>
			</staticText>
			<staticText>
				<reportElement x="27" y="2263" width="527" height="14"/>
