<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<description>Controller-controller dari modul U/W REINSTATE</description>

	<bean id="elions.uw_reinstate.editReinstateFormController" class="com.ekalife.elions.web.uw_reinstate.EditReinstateFormController" parent="parentFormController">
		<property name="formView" value="uw_reinstate/edit_report" />
	</bean>

	<bean id="elions.uw_reinstate.reinstateMultiController" class="com.ekalife.elions.web.uw_reinstate.ReinstateMultiController" parent="parentMultiController" />

	<bean id="elions.uw_reinstate.reinsReasFormController" class="com.ekalife.elions.web.uw_reinstate.ReinsReasFormController" parent="parentFormController">
		<property name="sessionForm" value="true" />
		<property name="formView" value="uw_reinstate/reins_reas" />
	</bean>

	<bean id="elions.uw_reinstate.reinstatementWorkSheetFormController" class="com.ekalife.elions.web.uw_reinstate.ReinstatementWorkSheetFormController" parent="parentFormController">
		<property name="sessionForm" value="true" />
		<property name="formView" value="uw_reinstate/reinstatement_work_sheet" />
	</bean>

</beans>
